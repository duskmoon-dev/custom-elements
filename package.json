{
  "name": "@duskmoon-dev/custom-elements",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": ["elements/*", "packages/*"],
  "scripts": {
    "build": "bun run --filter '*' build",
    "dev": "bun run --filter '*' dev",
    "lint": "bun run --filter '*' lint",
    "test": "bun run --filter '*' test",
    "typecheck": "bun run --filter '*' typecheck",
    "clean": "bun run --filter '*' clean",
    "build:core": "bun run --filter @duskmoon-dev/el-core build",
    "build:elements": "bun run --filter '@duskmoon-dev/el-*' --filter '!@duskmoon-dev/el-core' build"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "typescript": "^5.7.2"
  }
}
